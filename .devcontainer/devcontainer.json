{
  "name": "React Native + Expo",
  "dockerComposeFile": [
    "../compose.yml"
  ],
  "service": "react-react-native-app",
  "workspaceFolder": "/usr/src/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "msjsdiag.vscode-react-native",
        "esbenp.prettier-vscode"
      ]
